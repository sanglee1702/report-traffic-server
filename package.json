{
  "name": "report-traffic-server",
  "version": "1.0.0",
  "description": "Server rest full API for GoFun",
  "main": "dist/index.ts",
  "scripts": {
    "start": "nodemon -r dotenv/config src/index.ts dotenv_config_path=.env.dev",
    "start:dist": "nodemon -r dotenv/config dist/index.js dotenv_config_path=.env.dev",
    "staging": "nodemon -r dotenv/config src/index.ts dotenv_config_path=.env",
    "serve": "nodemon src/index.ts",
    "watch": "rm -r dist && tsc -w",
    "transpile": "rm -r dist && tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start:server": "nodemon -r dotenv/config dist/index.js dotenv_config_path=.env.prod",
    "initdata": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "express",
    "bcryptjs",
    "jsonwebtoken",
    "dotenv",
    "sequelize",
    "mysql2",
    "winston",
    "prettier",
    "typescript",
    "eslint",
    "swagger-ui-express",
    "axios"
  ],
  "author": "lenvo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.0",
    "hybrid-crypto-js": "^0.2.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.6.2",
    "path": "^0.12.7",
    "qs": "^6.10.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/eslint": "^7.2.13",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.7",
    "@types/node-rsa": "^1.1.1",
    "@types/nodemailer": "^6.4.2",
    "@types/sequelize": "^4.28.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
